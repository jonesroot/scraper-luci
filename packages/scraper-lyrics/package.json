{
    "name": "@jonesroot/scraper-lyrics",
    "version": "5.0.0",
    "description": "Lyrics scraper module",
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/@types/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js",
            "types": "./lib/@types/index.d.ts"
        }
    },
    "scripts": {
        "test": "npm run test:cjs & npm run test:esm",
        "test:cjs": "node --test ./lib/cjs/test/",
        "test:esm": "node --test ./lib/esm/test/",
        "build": "npm run build:cjs && npm run build:esm && npm run build:types",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build:types": "tsc -p tsconfig.types.json",
        "compile": "node ../../scripts/compile-typescript.js && npm run write:package",
        "write:package": "node ../../scripts/write-package.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jonesroot/scraper.git"
    },
    "author": "jonesroot",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/jonesroot/scraper/issues"
    },
    "homepage": "https://github.com/jonesroot/scraper/tree/main/packages/scraper-lyrics#readme",
    "files": [
        "lib/**/*.js",
        "lib/**/**.d.ts",
        "lib/**/package.json"
    ],
    "dependencies": {
        "cheerio": "^1.0.0-rc.12",
        "got": "^11.8.6",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@types/node": "^20.14.9"
    }
}
